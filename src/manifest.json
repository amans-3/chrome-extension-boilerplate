{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Extension Boilerplate",
  "description": "React + Tailwindcss + Javascript",
  "author": "example@gmail.com",
  "background": { "service_worker": "background.bundle.js" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icons/icon16.png",
      "32": "./icons/icon32.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  }
}
